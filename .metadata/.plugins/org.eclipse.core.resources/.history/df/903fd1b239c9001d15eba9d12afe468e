package binary;

public class ImplicitConversion {

	public static void main(String[] args) {
    //묵시적 형변환 예시
	
	byte bNum = 10;
	int num = bNum;
		System.out.println(num);
		
	long lNUM = 10;
	float fNUM = lNUM;
	
	System.out.println(fNUM);
	
	
	//묵시적 형변환 해석 : 작은수에서 큰수로 or 덜 정밀한 수에서 더 정밀한 수로 대입되는 경우  
	//                 정수에서 실수로 
	
	
	
	}

}
