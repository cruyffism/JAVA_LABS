package binary;

public class DoubleEx {
	

	public static void main(String[] args) {
    
    double dNum = 3.14D;
    float fNum = 3.14F;
    
    System.out.println(dNum);
    System.out.println(fNum);
    
  
    
    
    //var dNum =3.14; var은 지역 변수에서만 사용 가능(자료값을 보고 컴파일러가 추론하는 것)
    // var num = 10;   
    // num = 3.14;   >> 이미 인트로 선언 되어 있어서 이렇게 다시 선언은 불가 
    
    
		
	}

}
